<!DOCTYPE html>
<html lang="en">

{{> head}}


<body>
    {{{body}}}
    {{> header}}

    <div class="days">
        <li class="d1">Day 1</li>
        <li class="d2">Day 2</li>
        <li class="d3">Day 3</li>
        <li class="d4">Day 4</li>
        <li class="d5">Day 5</li>
        <li class="d6">Day 6</li>
        <li class="d7">Day 7</li>
    </div>

    <div class="content">
        <table class="w1">
            <thead>
                <tr>
                    <th>EXERCISE</th>
                    <th>SETS</th>
                    <th>TIME</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>{{exercise}}</td>
                    <td>{{sets}}</td>
                    <td>{{time}} seconds</td>
                </tr>
                <tr>
                    <td>Single leg Romanian Deadlift</td>
                    <td>4</td>
                    <td>60 seconds</td>
                </tr>
                <tr>
                    <td>Wall Sits</td>
                    <td>4</td>
                    <td>60 seconds</td>
                </tr>

                <tr>
                    <td>Push up with shoulder taps</td>
                    <td>4</td>
                    <td>60 seconds</td>
                </tr>

                <tr>
                    <td>Russian Twists</td>
                    <td>4</td>
                    <td>60 seconds</td>
                </tr>
            </tbody>
        </table>
    </div>

    {{!-- <script type="text/javascript" src="../Controller/animation.js"></script> --}}
</body>

</html>