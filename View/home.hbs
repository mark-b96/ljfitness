<!DOCTYPE html>
<html lang="en">

{{> head}}

<body>
    {{> header}}

    <div class="content">
        {{#each workouts}}
        <h1 onclick="location.href='/{{Day}}'">Day {{Day}}</h1>
        <table class="w1">


            <thead>
                <tr>
                    <th>EXERCISES</th>
                    <th>SETS</th>
                    <th>REPS</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    {{#each w1}}
                    <td>{{this}}</td>
                    {{/each}}
                </tr>
                <tr>
                    {{#each w2}}
                    <td>{{this}}</td>
                    {{/each}}
                </tr>
                <tr>
                    {{#each w3}}
                    <td>{{this}}</td>
                    {{/each}}
                </tr>
                <tr>
                    {{#each w4}}
                    <td>{{this}}</td>
                    {{/each}}
                </tr>
                <tr>
                    {{#each w5}}
                    <td>{{this}}</td>
                    {{/each}}
                </tr>

            </tbody>

        </table>
        {{/each}}
    </div>
    <script type="text/javascript" src="/Controller/menu.js"></script>
</body>